{% extends 'base.html.twig'%}

{% block body %}
<div class="card card-outline-secondary">
    <table class="table table-bordered" id="myTable">
        <thead class="thead-dark">
            <tr>
                <th>Matricule</th>
                <th>Prenom</th>
                <th>Nom</th>
                <th>Date de Naissance</th>
                <th>Telephone</th>
                <th>Email</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
        {% for etudiant in etudiants %}
            <tr>
                <td>{{etudiant.Matricule}}</td>
                <td>{{etudiant.Prenom}}</td>
                <td>{{etudiant.Nom}}</td>
                <td>{{etudiant.datedenaissance|date('d/m/y')}}</td>
                <td>{{etudiant.Telephone}}</td>
                <td>{{etudiant.email}}</td>
                {# <td>
                <a href="#"><span class="iconify" data-icon="bx:bxs-edit" data-inline="false" style="color: #454545;"></span></a>
                <a href="#"><span class="iconify" data-icon="carbon:delete" data-inline="false"></span></a>
                </td>      #}
            </tr>      
        {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}